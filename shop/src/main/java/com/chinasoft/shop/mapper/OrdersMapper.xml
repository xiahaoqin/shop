<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chinasoft.shop.mapper.OrdersMapper" >
  <insert id="insert" parameterType="com.chinasoft.shop.pojo.Orders" >
    insert into orders (id, time, uname, 
      payWay, sendWay, totalMoney, 
      state, submit)
    values (#{id,jdbcType=VARCHAR}, #{time,jdbcType=TIMESTAMP}, #{uname,jdbcType=VARCHAR}, 
      #{payway,jdbcType=VARCHAR}, #{sendway,jdbcType=VARCHAR}, #{totalmoney,jdbcType=DOUBLE}, 
      #{state,jdbcType=CHAR}, #{submit,jdbcType=CHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.chinasoft.shop.pojo.Orders" >
    insert into orders
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        id,
      </if>
      <if test="time != null" >
        time,
      </if>
      <if test="uname != null" >
        uname,
      </if>
      <if test="payway != null" >
        payWay,
      </if>
      <if test="sendway != null" >
        sendWay,
      </if>
      <if test="totalmoney != null" >
        totalMoney,
      </if>
      <if test="state != null" >
        state,
      </if>
      <if test="submit != null" >
        submit,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        #{id,jdbcType=VARCHAR},
      </if>
      <if test="time != null" >
        #{time,jdbcType=TIMESTAMP},
      </if>
      <if test="uname != null" >
        #{uname,jdbcType=VARCHAR},
      </if>
      <if test="payway != null" >
        #{payway,jdbcType=VARCHAR},
      </if>
      <if test="sendway != null" >
        #{sendway,jdbcType=VARCHAR},
      </if>
      <if test="totalmoney != null" >
        #{totalmoney,jdbcType=DOUBLE},
      </if>
      <if test="state != null" >
        #{state,jdbcType=CHAR},
      </if>
      <if test="submit != null" >
        #{submit,jdbcType=CHAR},
      </if>
    </trim>
  </insert>
</mapper>